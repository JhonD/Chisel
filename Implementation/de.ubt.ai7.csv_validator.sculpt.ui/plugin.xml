<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>

<plugin>

    <extension
            point="org.eclipse.ui.editors">
        <editor
            class="de.ubt.ai7.csv_validator.ui.SculptExecutableExtensionFactory:org.eclipse.xtext.ui.editor.XtextEditor"
            contributorClass="org.eclipse.ui.editors.text.TextEditorActionContributor"
            default="true"
            extensions="sculpt"
            id="de.ubt.ai7.csv_validator.Sculpt"
            name="Sculpt Editor">
        </editor>
    </extension>
    <extension
        point="org.eclipse.ui.handlers">
        <handler
            class="de.ubt.ai7.csv_validator.ui.SculptExecutableExtensionFactory:org.eclipse.xtext.ui.editor.hyperlinking.OpenDeclarationHandler"
            commandId="org.eclipse.xtext.ui.editor.hyperlinking.OpenDeclaration">
            <activeWhen>
                <reference
                    definitionId="de.ubt.ai7.csv_validator.Sculpt.Editor.opened">
                </reference>
            </activeWhen>
        </handler>
        <handler
            class="de.ubt.ai7.csv_validator.ui.SculptExecutableExtensionFactory:org.eclipse.xtext.ui.editor.handler.ValidateActionHandler"
            commandId="de.ubt.ai7.csv_validator.Sculpt.validate">
         <activeWhen>
            <reference
                    definitionId="de.ubt.ai7.csv_validator.Sculpt.Editor.opened">
            </reference>
         </activeWhen>
      	</handler>
      	<!-- copy qualified name -->
        <handler
            class="de.ubt.ai7.csv_validator.ui.SculptExecutableExtensionFactory:org.eclipse.xtext.ui.editor.copyqualifiedname.EditorCopyQualifiedNameHandler"
            commandId="org.eclipse.xtext.ui.editor.copyqualifiedname.EditorCopyQualifiedName">
            <activeWhen>
				<reference definitionId="de.ubt.ai7.csv_validator.Sculpt.Editor.opened" />
            </activeWhen>
        </handler>
        <handler
            class="de.ubt.ai7.csv_validator.ui.SculptExecutableExtensionFactory:org.eclipse.xtext.ui.editor.copyqualifiedname.OutlineCopyQualifiedNameHandler"
            commandId="org.eclipse.xtext.ui.editor.copyqualifiedname.OutlineCopyQualifiedName">
            <activeWhen>
            	<and>
            		<reference definitionId="de.ubt.ai7.csv_validator.Sculpt.XtextEditor.opened" />
	                <iterate>
						<adapt type="org.eclipse.xtext.ui.editor.outline.IOutlineNode" />
					</iterate>
				</and>
            </activeWhen>
        </handler>
    </extension>
    <extension point="org.eclipse.core.expressions.definitions">
        <definition id="de.ubt.ai7.csv_validator.Sculpt.Editor.opened">
            <and>
                <reference definitionId="isActiveEditorAnInstanceOfXtextEditor"/>
                <with variable="activeEditor">
                    <test property="org.eclipse.xtext.ui.editor.XtextEditor.languageName" 
                        value="de.ubt.ai7.csv_validator.Sculpt" 
                        forcePluginActivation="true"/>
                </with>        
            </and>
        </definition>
        <definition id="de.ubt.ai7.csv_validator.Sculpt.XtextEditor.opened">
            <and>
                <reference definitionId="isXtextEditorActive"/>
                <with variable="activeEditor">
                    <test property="org.eclipse.xtext.ui.editor.XtextEditor.languageName" 
                        value="de.ubt.ai7.csv_validator.Sculpt" 
                        forcePluginActivation="true"/>
                </with>        
            </and>
        </definition>
    </extension>
    <extension
            point="org.eclipse.ui.preferencePages">
        <page
            class="de.ubt.ai7.csv_validator.ui.SculptExecutableExtensionFactory:org.eclipse.xtext.ui.editor.preferences.LanguageRootPreferencePage"
            id="de.ubt.ai7.csv_validator.Sculpt"
            name="Sculpt">
            <keywordReference id="de.ubt.ai7.csv_validator.ui.keyword_Sculpt"/>
        </page>
        <page
            category="de.ubt.ai7.csv_validator.Sculpt"
            class="de.ubt.ai7.csv_validator.ui.SculptExecutableExtensionFactory:org.eclipse.xtext.ui.editor.syntaxcoloring.SyntaxColoringPreferencePage"
            id="de.ubt.ai7.csv_validator.Sculpt.coloring"
            name="Syntax Coloring">
            <keywordReference id="de.ubt.ai7.csv_validator.ui.keyword_Sculpt"/>
        </page>
        <page
            category="de.ubt.ai7.csv_validator.Sculpt"
            class="de.ubt.ai7.csv_validator.ui.SculptExecutableExtensionFactory:org.eclipse.xtext.ui.editor.templates.XtextTemplatePreferencePage"
            id="de.ubt.ai7.csv_validator.Sculpt.templates"
            name="Templates">
            <keywordReference id="de.ubt.ai7.csv_validator.ui.keyword_Sculpt"/>
        </page>
    </extension>
    <extension
            point="org.eclipse.ui.propertyPages">
        <page
            class="de.ubt.ai7.csv_validator.ui.SculptExecutableExtensionFactory:org.eclipse.xtext.ui.editor.preferences.LanguageRootPreferencePage"
            id="de.ubt.ai7.csv_validator.Sculpt"
            name="Sculpt">
            <keywordReference id="de.ubt.ai7.csv_validator.ui.keyword_Sculpt"/>
            <enabledWhen>
	            <adapt type="org.eclipse.core.resources.IProject"/>
			</enabledWhen>
	        <filter name="projectNature" value="org.eclipse.xtext.ui.shared.xtextNature"/>
        </page>
    </extension>
    <extension
        point="org.eclipse.ui.keywords">
        <keyword
            id="de.ubt.ai7.csv_validator.ui.keyword_Sculpt"
            label="Sculpt"/>
    </extension>
    <extension
         point="org.eclipse.ui.commands">
      <command
            description="Trigger expensive validation"
            id="de.ubt.ai7.csv_validator.Sculpt.validate"
            name="Validate">
      </command>
      <!-- copy qualified name -->
      <command
            id="org.eclipse.xtext.ui.editor.copyqualifiedname.EditorCopyQualifiedName"
            categoryId="org.eclipse.ui.category.edit"
            description="Copy the qualified name for the selected element"
            name="Copy Qualified Name">
      </command>
      <command
            id="org.eclipse.xtext.ui.editor.copyqualifiedname.OutlineCopyQualifiedName"
            categoryId="org.eclipse.ui.category.edit"
            description="Copy the qualified name for the selected element"
            name="Copy Qualified Name">
      </command>
    </extension>
    <extension point="org.eclipse.ui.menus">
        <menuContribution
            locationURI="popup:#TextEditorContext?after=group.edit">
             <command
                 commandId="de.ubt.ai7.csv_validator.Sculpt.validate"
                 style="push"
                 tooltip="Trigger expensive validation">
            <visibleWhen checkEnabled="false">
                <reference
                    definitionId="de.ubt.ai7.csv_validator.Sculpt.Editor.opened">
                </reference>
            </visibleWhen>
         </command>  
         </menuContribution>
         <!-- copy qualified name -->
         <menuContribution locationURI="popup:#TextEditorContext?after=copy">
         	<command commandId="org.eclipse.xtext.ui.editor.copyqualifiedname.EditorCopyQualifiedName" 
         		style="push" tooltip="Copy Qualified Name">
            	<visibleWhen checkEnabled="false">
                	<reference definitionId="de.ubt.ai7.csv_validator.Sculpt.Editor.opened" />
            	</visibleWhen>
         	</command>  
         </menuContribution>
         <menuContribution locationURI="menu:edit?after=copy">
         	<command commandId="org.eclipse.xtext.ui.editor.copyqualifiedname.EditorCopyQualifiedName"
            	style="push" tooltip="Copy Qualified Name">
            	<visibleWhen checkEnabled="false">
                	<reference definitionId="de.ubt.ai7.csv_validator.Sculpt.Editor.opened" />
            	</visibleWhen>
         	</command>  
         </menuContribution>
         <menuContribution locationURI="popup:org.eclipse.xtext.ui.outline?after=additions">
			<command commandId="org.eclipse.xtext.ui.editor.copyqualifiedname.OutlineCopyQualifiedName" 
				style="push" tooltip="Copy Qualified Name">
         		<visibleWhen checkEnabled="false">
	            	<and>
	            		<reference definitionId="de.ubt.ai7.csv_validator.Sculpt.XtextEditor.opened" />
						<iterate>
							<adapt type="org.eclipse.xtext.ui.editor.outline.IOutlineNode" />
						</iterate>
					</and>
				</visibleWhen>
			</command>
         </menuContribution>
    </extension>
    <extension point="org.eclipse.ui.menus">
		<menuContribution locationURI="popup:#TextEditorContext?endof=group.find">
			<command commandId="org.eclipse.xtext.ui.editor.FindReferences">
				<visibleWhen checkEnabled="false">
                	<reference definitionId="de.ubt.ai7.csv_validator.Sculpt.Editor.opened">
                	</reference>
            	</visibleWhen>
			</command>
		</menuContribution>
	</extension>
	<extension point="org.eclipse.ui.handlers">
	    <handler
            class="de.ubt.ai7.csv_validator.ui.SculptExecutableExtensionFactory:org.eclipse.xtext.ui.editor.findrefs.FindReferencesHandler"
            commandId="org.eclipse.xtext.ui.editor.FindReferences">
            <activeWhen>
                <reference
                    definitionId="de.ubt.ai7.csv_validator.Sculpt.Editor.opened">
                </reference>
            </activeWhen>
        </handler>
    </extension>   

<!-- adding resource factories -->

	<extension
		point="org.eclipse.emf.ecore.extension_parser">
		<parser
			class="de.ubt.ai7.csv_validator.ui.SculptExecutableExtensionFactory:org.eclipse.xtext.resource.IResourceFactory"
			type="sculpt">
		</parser>
	</extension>
	<extension point="org.eclipse.xtext.extension_resourceServiceProvider">
        <resourceServiceProvider
            class="de.ubt.ai7.csv_validator.ui.SculptExecutableExtensionFactory:org.eclipse.xtext.ui.resource.IResourceUIServiceProvider"
            uriExtension="sculpt">
        </resourceServiceProvider>
    </extension>


	<!-- marker definitions for de.ubt.ai7.csv_validator.Sculpt -->
	<extension
	        id="sculpt.check.fast"
	        name="Sculpt Problem"
	        point="org.eclipse.core.resources.markers">
	    <super type="org.eclipse.xtext.ui.check.fast"/>
	    <persistent value="true"/>
	</extension>
	<extension
	        id="sculpt.check.normal"
	        name="Sculpt Problem"
	        point="org.eclipse.core.resources.markers">
	    <super type="org.eclipse.xtext.ui.check.normal"/>
	    <persistent value="true"/>
	</extension>
	<extension
	        id="sculpt.check.expensive"
	        name="Sculpt Problem"
	        point="org.eclipse.core.resources.markers">
	    <super type="org.eclipse.xtext.ui.check.expensive"/>
	    <persistent value="true"/>
	</extension>

   <extension
         point="org.eclipse.xtext.builder.participant">
      <participant
            class="de.ubt.ai7.csv_validator.ui.SculptExecutableExtensionFactory:org.eclipse.xtext.builder.IXtextBuilderParticipant"
            fileExtensions="sculpt"
            >
      </participant>
   </extension>
   <extension
            point="org.eclipse.ui.preferencePages">
        <page
            category="de.ubt.ai7.csv_validator.Sculpt"
            class="de.ubt.ai7.csv_validator.ui.SculptExecutableExtensionFactory:org.eclipse.xtext.builder.preferences.BuilderPreferencePage"
            id="de.ubt.ai7.csv_validator.Sculpt.compiler.preferencePage"
            name="Compiler">
            <keywordReference id="de.ubt.ai7.csv_validator.ui.keyword_Sculpt"/>
        </page>
    </extension>
    <extension
            point="org.eclipse.ui.propertyPages">
        <page
            category="de.ubt.ai7.csv_validator.Sculpt"
            class="de.ubt.ai7.csv_validator.ui.SculptExecutableExtensionFactory:org.eclipse.xtext.builder.preferences.BuilderPreferencePage"
            id="de.ubt.ai7.csv_validator.Sculpt.compiler.propertyPage"
            name="Compiler">
            <keywordReference id="de.ubt.ai7.csv_validator.ui.keyword_Sculpt"/>
            <enabledWhen>
	            <adapt type="org.eclipse.core.resources.IProject"/>
			</enabledWhen>
	        <filter name="projectNature" value="org.eclipse.xtext.ui.shared.xtextNature"/>
        </page>
    </extension>
    <extension point="org.eclipse.ui.menus">
		<menuContribution locationURI="popup:#TextEditorContext?after=xtext.ui.openDeclaration">
			<command
				commandId="org.eclipse.xtext.ui.OpenGeneratedFileCommand"
				id="de.ubt.ai7.csv_validator.Sculpt.OpenGeneratedCode"
				style="push">
					<visibleWhen checkEnabled="false">
						<reference definitionId="de.ubt.ai7.csv_validator.Sculpt.Editor.opened" />
					</visibleWhen>
			</command>
		</menuContribution>
	</extension>
	<extension point="org.eclipse.ui.handlers">
		<handler
			class="de.ubt.ai7.csv_validator.ui.SculptExecutableExtensionFactory:org.eclipse.xtext.ui.generator.trace.OpenGeneratedFileHandler"
			commandId="org.eclipse.xtext.ui.OpenGeneratedFileCommand">
				<activeWhen>
					<reference definitionId="de.ubt.ai7.csv_validator.Sculpt.Editor.opened" />
				</activeWhen>
		</handler>
	</extension>

	<!-- Quick Outline -->
	<extension
		point="org.eclipse.ui.handlers">
		<handler 
			class="de.ubt.ai7.csv_validator.ui.SculptExecutableExtensionFactory:org.eclipse.xtext.ui.editor.outline.quickoutline.ShowQuickOutlineActionHandler"
			commandId="org.eclipse.xtext.ui.editor.outline.QuickOutline">
			<activeWhen>
				<reference
					definitionId="de.ubt.ai7.csv_validator.Sculpt.Editor.opened">
				</reference>
			</activeWhen>
		</handler>
	</extension>
	<extension
		point="org.eclipse.ui.commands">
		<command
			description="Open the quick outline."
			id="org.eclipse.xtext.ui.editor.outline.QuickOutline"
			name="Quick Outline">
		</command>
	</extension>
	<extension point="org.eclipse.ui.menus">
		<menuContribution
			locationURI="popup:#TextEditorContext?after=group.open">
			<command commandId="org.eclipse.xtext.ui.editor.outline.QuickOutline"
				style="push"
				tooltip="Open Quick Outline">
				<visibleWhen checkEnabled="false">
					<reference definitionId="de.ubt.ai7.csv_validator.Sculpt.Editor.opened"/>
				</visibleWhen>
			</command>
		</menuContribution>
	</extension>
    <!-- quickfix marker resolution generator for de.ubt.ai7.csv_validator.Sculpt -->
    <extension
            point="org.eclipse.ui.ide.markerResolution">
        <markerResolutionGenerator
            class="de.ubt.ai7.csv_validator.ui.SculptExecutableExtensionFactory:org.eclipse.xtext.ui.editor.quickfix.MarkerResolutionGenerator"
            markerType="de.ubt.ai7.csv_validator.ui.sculpt.check.fast">
            <attribute
                name="FIXABLE_KEY"
                value="true">
            </attribute>
        </markerResolutionGenerator>
        <markerResolutionGenerator
            class="de.ubt.ai7.csv_validator.ui.SculptExecutableExtensionFactory:org.eclipse.xtext.ui.editor.quickfix.MarkerResolutionGenerator"
            markerType="de.ubt.ai7.csv_validator.ui.sculpt.check.normal">
            <attribute
                name="FIXABLE_KEY"
                value="true">
            </attribute>
        </markerResolutionGenerator>
        <markerResolutionGenerator
            class="de.ubt.ai7.csv_validator.ui.SculptExecutableExtensionFactory:org.eclipse.xtext.ui.editor.quickfix.MarkerResolutionGenerator"
            markerType="de.ubt.ai7.csv_validator.ui.sculpt.check.expensive">
            <attribute
                name="FIXABLE_KEY"
                value="true">
            </attribute>
        </markerResolutionGenerator>
    </extension>
   	<!-- Rename Refactoring -->
	<extension point="org.eclipse.ui.handlers">
		<handler 
			class="de.ubt.ai7.csv_validator.ui.SculptExecutableExtensionFactory:org.eclipse.xtext.ui.refactoring.ui.DefaultRenameElementHandler"
			commandId="org.eclipse.xtext.ui.refactoring.RenameElement">
			<activeWhen>
				<reference
					definitionId="de.ubt.ai7.csv_validator.Sculpt.Editor.opened">
				</reference>
			</activeWhen>
		</handler>
	</extension>
    <extension point="org.eclipse.ui.menus">
         <menuContribution
            locationURI="popup:#TextEditorContext?after=group.edit">
         <command commandId="org.eclipse.xtext.ui.refactoring.RenameElement"
               style="push">
            <visibleWhen checkEnabled="false">
               <reference
                     definitionId="de.ubt.ai7.csv_validator.Sculpt.Editor.opened">
               </reference>
            </visibleWhen>
         </command>
      </menuContribution>
   </extension>
   <extension point="org.eclipse.ui.preferencePages">
	    <page
	        category="de.ubt.ai7.csv_validator.Sculpt"
	        class="de.ubt.ai7.csv_validator.ui.SculptExecutableExtensionFactory:org.eclipse.xtext.ui.refactoring.ui.RefactoringPreferencePage"
	        id="de.ubt.ai7.csv_validator.Sculpt.refactoring"
	        name="Refactoring">
	        <keywordReference id="de.ubt.ai7.csv_validator.ui.keyword_Sculpt"/>
	    </page>
	</extension>

  <extension point="org.eclipse.compare.contentViewers">
    <viewer id="de.ubt.ai7.csv_validator.Sculpt.compare.contentViewers"
            class="de.ubt.ai7.csv_validator.ui.SculptExecutableExtensionFactory:org.eclipse.xtext.ui.compare.InjectableViewerCreator"
            extensions="sculpt">
    </viewer>
  </extension>
  <extension point="org.eclipse.compare.contentMergeViewers">
    <viewer id="de.ubt.ai7.csv_validator.Sculpt.compare.contentMergeViewers"
            class="de.ubt.ai7.csv_validator.ui.SculptExecutableExtensionFactory:org.eclipse.xtext.ui.compare.InjectableViewerCreator"
            extensions="sculpt" label="Sculpt Compare">
     </viewer>
  </extension>
  <extension point="org.eclipse.ui.editors.documentProviders">
    <provider id="de.ubt.ai7.csv_validator.Sculpt.editors.documentProviders"
            class="de.ubt.ai7.csv_validator.ui.SculptExecutableExtensionFactory:org.eclipse.xtext.ui.editor.model.XtextDocumentProvider"
            extensions="sculpt">
    </provider>
  </extension>
  <extension point="org.eclipse.team.core.fileTypes">
    <fileTypes
            extension="sculpt"
            type="text">
    </fileTypes>
  </extension>
  <!-- Right-click menu -->
  <extension
        point="org.eclipse.ui.handlers">
     <handler
           class="de.ubt.ai7.csv_validator.ui.SculptExecutableExtensionFactory:de.ubt.ai7.csv_validator.sculpt.ui.commands.handler.ValidationHandler"
           commandId="de.ubt.ai7.csv_validator.sculpt.ui.commands.ValidateSculptCommand">
     </handler>
     <handler
           class="de.ubt.ai7.csv_validator.ui.SculptExecutableExtensionFactory:de.ubt.ai7.csv_validator.sculpt.ui.commands.handler.ClearCachesHandler"
           commandId="de.ubt.ai7.csv_validator.sculpt.ui.commands.ClearCachesCommand">
     </handler>
     <handler
           class="de.ubt.ai7.csv_validator.ui.SculptExecutableExtensionFactory:de.ubt.ai7.csv_validator.sculpt.ui.commands.handler.RegisterSchemaHandler"
           commandId="de.ubt.ai7.csv_validator.sculpt.ui.commands.RegisterSchemaCommand">
     </handler>
      
  </extension>
   
  <extension
        point="org.eclipse.ui.commands">
        <command
              id="de.ubt.ai7.csv_validator.sculpt.ui.commands.RegisterSchemaCommand"
              name="Load Sculpt Schema">
        </command>
        <command
              id="de.ubt.ai7.csv_validator.sculpt.ui.commands.ValidateSculptCommand"
              name="Validate registered Csv">
        </command>
        <command
              id="de.ubt.ai7.csv_validator.sculpt.ui.commands.ClearCachesCommand"
              name="Clear caches">
        </command>
  </extension>
    
  <extension point="org.eclipse.ui.menus">
    <menuContribution locationURI="popup:#TextEditorContext">
        <menu
              label="Sculpt">
           <command
                 commandId="de.ubt.ai7.csv_validator.sculpt.ui.commands.RegisterSchemaCommand"
                 label="Load Sculpt Schema"
                 style="push">
              <visibleWhen
                    checkEnabled="false">
                 <reference
                       definitionId="de.ubt.ai7.csv_validator.Sculpt.Editor.opened">
                 </reference>
              </visibleWhen>
           </command>
           <command
                 commandId="de.ubt.ai7.csv_validator.sculpt.ui.commands.ValidateSculptCommand"
                 label="Validate registered Csv"
                 style="push">
              <visibleWhen
                    checkEnabled="false">
                 <reference
                       definitionId="de.ubt.ai7.csv_validator.Sculpt.Editor.opened">
                 </reference>
              </visibleWhen>
           </command>
           <command
                 commandId="de.ubt.ai7.csv_validator.sculpt.ui.commands.ClearCachesCommand"
                 label="Clear Caches"
                 style="push">
              <visibleWhen
                    checkEnabled="false">
                 <reference
                       definitionId="de.ubt.ai7.csv_validator.Sculpt.Editor.opened">
                 </reference>
              </visibleWhen>
           </command>
        </menu>
    </menuContribution>
  </extension>
  <!-- Custom markers -->
  <extension
    id="de.ubt.ai7.csv_validation.sculptMarker"
    name="Sculpt Marker"
    point="org.eclipse.core.resources.markers">
  <super
    type="org.eclipse.core.resources.textmarker">
  </super>
  </extension>
  <extension
        point="org.eclipse.ui.editors.annotationTypes">
     <type
           markerType="de.ubt.ai7.csv_validation.sculptMarker"
           name="de.ubt.ai7.csv_validation.sculptMarker">
     </type>
  </extension>
  <extension
        point="org.eclipse.ui.editors.markerAnnotationSpecification">
     <specification
           annotationType="de.ubt.ai7.csv_validation.sculptMarker"
           colorPreferenceKey="de.ubt.ai7.csv_validation.sculptMarker.color"
           colorPreferenceValue="151,255,151"
           contributesToHeader="false"
           highlightPreferenceKey="de.ubt.ai7.csv_validation.sculptMarker.highlight"
           highlightPreferenceValue="true"
           includeOnPreferencePage="true"
           label="Sculpt Marker"
           overviewRulerPreferenceKey="de.ubt.ai7.csv_validation.sculptMarker.overview"
           overviewRulerPreferenceValue="true"
           presentationLayer="0"
           textPreferenceKey="de.ubt.ai7.csv_validation.sculptMarker.text"
           textPreferenceValue="true"
           textStylePreferenceValue="BOX"
           verticalRulerPreferenceKey="de.ubt.ai7.csv_validation.sculptMarker.ruler"
           verticalRulerPreferenceValue="true">
     </specification>
   </extension>
  <extension
    id="de.ubt.ai7.csv_validation.sculptErrorMarker"
    name="Sculpt Error Marker"
    point="org.eclipse.core.resources.markers">
  <super
    type="org.eclipse.core.resources.textmarker">
  </super>
  </extension>
  <extension
        point="org.eclipse.ui.editors.annotationTypes">
     <type
           markerType="de.ubt.ai7.csv_validation.sculptErrorMarker"
           name="de.ubt.ai7.csv_validation.sculptErrorMarker">
     </type>
  </extension>
  <extension
        point="org.eclipse.ui.editors.markerAnnotationSpecification">
     <specification
           annotationType="de.ubt.ai7.csv_validation.sculptErrorMarker"
           colorPreferenceKey="de.ubt.ai7.csv_validation.sculptErrorMarker.color"
           colorPreferenceValue="255,151,151"
           contributesToHeader="false"
           highlightPreferenceKey="de.ubt.ai7.csv_validation.sculptErrorMarker.highlight"
           highlightPreferenceValue="true"
           includeOnPreferencePage="true"
           label="Sculpt Error Marker"
           overviewRulerPreferenceKey="de.ubt.ai7.csv_validation.sculptErrorMarker.overview"
           overviewRulerPreferenceValue="true"
           presentationLayer="0"
           textPreferenceKey="de.ubt.ai7.csv_validation.sculptErrorMarker.text"
           textPreferenceValue="true"
           textStylePreferenceValue="BOX"
           verticalRulerPreferenceKey="de.ubt.ai7.csv_validation.sculptErrorMarker.ruler"
           verticalRulerPreferenceValue="true">
     </specification>
   </extension>
</plugin>
